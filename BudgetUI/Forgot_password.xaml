<Window x:Class="BudgetUI.Forgot_password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BudgetUI"
        mc:Ignorable="d"
        Title="Forgot_password" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="TextBlock_question" Margin="30,217,400,100" FontSize="30"></TextBlock>
        <TextBox x:Name="TextBlock_login" Margin="400,95,30,222"></TextBox>
        <Button x:Name="Button_login_back" Margin="0,0,676,396" Click="Button_login_back_Click">Back to log in</Button>
    </Grid>
</Window>
